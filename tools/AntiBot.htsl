goto event "Player Join"
if (stat "AntiBot/Strikes" == 2) {
    tp "custom_coordinates" "" /* coords for auto-kicker */
} else {
    stat "AntiBot/PosX" = locX
    stat "AntiBot/PosZ" = locZ
}

goto event "Player Quit"
if (stat "SeshPlaytime" < 10, stat "AntiBot/PosX" == locX, stat "AntiBot/PosZ" == locZ) {
    stat "AntiBot/Strikes" += 1
}

// FINISHED IF %stat.player/x/z% WORKS